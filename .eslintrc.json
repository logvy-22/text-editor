{
  "extends": "airbnb",
  "plugins": [
    "babel",
    "react",
    "import"
  ],
  "env": {
    "browser" : true
  },
  "rules": {
    "indent": ["error", 2],
    "import/no-unresolved": "off",
    "react/jsx-filename-extension": "off",
    "jsx-quotes": ["error", "prefer-double"],
    "quotes": ["error", "single"],
    "react/jsx-indent": ["error", 2],
    "react/jsx-indent-props": ["error", 2],
    "jsx-a11y/no-static-element-interactions": 0
  }
}
